{
	"collectCoverageFrom": [
		"src/**/*.ts",
		"!src/Main.ts"
	],
	"transform": {
		"^.+\\.(t|j)sx?$": "ts-jest"
	},
	"testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
	"testEnvironment": "node",
	"moduleFileExtensions": [
		"ts",
		"tsx",
		"js",
		"jsx",
		"json",
		"node"
	],
	"coverageDirectory": "coverage"
}