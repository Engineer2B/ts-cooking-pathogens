    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  },
  "files": [
    "build/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Engineer2B/ts-cooking-pathogens.git"
  },
  "bugs": {
    "url": "https://github.com/Engineer2B/ts-cooking-pathogens/issues",
